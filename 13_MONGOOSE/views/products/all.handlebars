<h1 class="title">Produtos</h1>
<div class="products">
  {{#each products}}
    <div class="product">
      <a href="/products/{{this._id}}">
        <h3>{{this.name}}</h3>
        <img src="{{this.image}}" alt="{{this.name}}">
        <p>R${{this.price}}</p>
        <p>{{this.description}}</p>
      </a>
        <div class="actions">
            <a href="/products/edit/{{this._id}}">Editar</a>
        </div>
    </div>
  {{/each}}
</div>