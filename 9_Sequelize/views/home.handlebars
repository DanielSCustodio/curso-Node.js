<h1>Usuários cadastrados</h1>
<div class="container">
  {{#each users}}
    <div class="item">
      <p>Nome: {{this.name}}</p>
      <span>Profissão: {{this.occupation}}</span>
      <div class="actions">
        <a href="/users/{{this.id}}">Ver detalhes</a>
      </div>
    </div>
  {{/each}}
</div>
